{
  "name": "URL Append param",
  "description": "Append param to current url",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "contextMenus", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/url_16.png",
      "32": "/images/url_32.png",
      "48": "/images/url_48.png",
      "128": "/images/url_128.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "/images/url_16.png",
    "32": "/images/url_32.png",
    "48": "/images/url_48.png",
    "128": "/images/url_128.png"
  },
  "app": {
    "urls": ["https://zzuwzj.github.io/chrome-ext-url-add-param/"],
    "launch": {
      "web_url": "https://zzuwzj.github.io/chrome-ext-url-add-param/"
    }
  }
}
